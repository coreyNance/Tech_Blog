<div class="row" id= "loginSignup">
  
 
  <div class="col-md-20">
    <h2>Signup</h2>
       <form class="form signup-form">
  <div class="mb-3 form-group">
    <div class="mb-3 form-group">
    <label for="name-signup" class="form-label">Name</label>
    <input type="text" class="form-control" id="name-signup">
  </div>
    <label for="email-signup" class="form-label">Username (Email Address)</label>
    <input type="email" class="form-control" id="email-signup" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
  </div>
  <div class="mb-3 form-group">
    <label for="password-signup" class="form-label">Password</label>
    <input type="password" class="form-control" id="password-signup">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

 
  </div>
</div>

<script src="./js/login.js"></script>






















